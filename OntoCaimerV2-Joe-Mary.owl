    <!-- http://www.semanticweb.org/OntoCaimer/Caregiver -->

    <owl:Class rdf:about="http://www.semanticweb.org/OntoCaimer/Caregiver">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/OntoCaimer/Person"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/OntoCaimer/Patient"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/OntoCaimer/Changes_in_sleep_patterns -->

    <owl:Class rdf:about="http://www.semanticweb.org/OntoCaimer/Changes_in_sleep_patterns">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/OntoCaimer/Symptom"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/OntoCaimer/belongsTo"/>
                <owl:hasValue rdf:resource="http://www.semanticweb.org/OntoCaimer/Moderate"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/OntoCaimer/hasRecommendation"/>
                <owl:hasValue rdf:resource="http://www.semanticweb.org/OntoCaimer/RMI4"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/OntoCaimer/hasRecommendation"/>
                <owl:hasValue rdf:resource="http://www.semanticweb.org/OntoCaimer/RMI5"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/OntoCaimer/hasRecommendation"/>
                <owl:hasValue rdf:resource="http://www.semanticweb.org/OntoCaimer/RMO1"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
