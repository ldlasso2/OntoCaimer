    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/OntoCaimer/RS52 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/OntoCaimer/RS52">
        <rdf:type rdf:resource="http://www.semanticweb.org/OntoCaimer/Recommendation"/>
