    </DataPropertyAssertion>
    <DataPropertyAssertion>
        <DataProperty IRI="/Examples"/>
        <NamedIndividual IRI="/RMI1"/>
        <Literal>Wall calendar, weekly planner</Literal>
    </DataPropertyAssertion>
    <DataPropertyAssertion>
        <DataProperty IRI="/DescriptionRecommendation"/>
        <NamedIndividual IRI="/RMI10"/>
        <Literal>Place a black mat or paint a black space on your front porch</Literal>
    </DataPropertyAssertion>
    <DataPropertyAssertion>
        <DataProperty IRI="/DescriptionRecommendation"/>
        <NamedIndividual IRI="/RMI11"/>
        <Literal>Add child-safe plastic covers to doorknobs.</Literal>
    </DataPropertyAssertion>
    <DataPropertyAssertion>
        <DataProperty IRI="/DescriptionRecommendation"/>
        <NamedIndividual IRI="/RMI12"/>
